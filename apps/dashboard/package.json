{
  "name": "@majoexe/dashboard",
  "version": "6.0.0",
  "description": "Dashboard for Majo.exe built on Next.js and Tailwind.css",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "deploy": "pnpm run build && pnpm run start",
    "format": "prettier --write . --cache --ignore-unknown",
    "format:check": "prettier --check --cache .",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@headlessui/react": "1.7.15",
    "@headlessui/tailwindcss": "0.1.3",
    "@heroicons/react": "2.0.18",
    "@igorkowalczyk/is-browser": "5.0.0",
    "@majoexe/database": "workspace:*",
    "@majoexe/util": "workspace:*",
    "@majoexe/config": "workspace:*",
    "@next-auth/prisma-adapter": "1.0.7",
    "@splinetool/react-spline": "^2.2.6",
    "@tippyjs/react": "4.2.6",
    "@vercel/analytics": "1.0.1",
    "@vercel/og": "0.5.8",
    "autoprefixer": "10.4.14",
    "clsx": "1.2.1",
    "globby": "13.2.2",
    "mini-svg-data-uri": "1.4.4",
    "next": "13.4.1",
    "next-auth": "4.22.1",
    "next-themes": "0.2.1",
    "node-fetch": "3.3.1",
    "nprogress": "0.2.0",
    "postcss": "8.4.25",
    "react": "18.2.0",
    "react-colorful": "5.6.1",
    "react-dom": "18.2.0",
    "react-infinite-scroller": "1.2.6",
    "react-table": "7.8.0",
    "react-twemoji": "0.5.0",
    "sharp": "0.32.1",
    "swr": "2.2.0",
    "tailwind-gradient-mask-image": "1.0.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-text-fill": "0.2.0"
  },
  "devDependencies": {
    "@majoexe/eslint-config": "workspace:*",
    "@next/bundle-analyzer": "13.4.1",
    "@prisma/nextjs-monorepo-workaround-plugin": "4.16.2",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.1",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0"
  },
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorkowalczyk/majo.exe.git",
    "directory": "apps/dashboard"
  },
  "keywords": [
    "Majo.exe",
    "Dashboard",
    "React",
    "Next.js",
    "Tailwind.css",
    "web-dashboard"
  ],
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igorkowalczyk/majo.exe/issues"
  },
  "homepage": "https://github.com/igorkowalczyk/majo.exe#readme",
  "packageManager": "pnpm@8.6.6"
}
