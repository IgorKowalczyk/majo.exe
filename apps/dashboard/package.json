    "cobe": "0.6.3",
    "discord-api-types": "0.37.119",
    "discord-giveaways": "6.0.1",
    "framer-motion": "12.0.11",
    "geist": "1.3.1",
    "js-file-download": "0.4.12",
    "json-2-csv": "5.5.8",
    "lucide-react": "0.474.0",
    "mini-svg-data-uri": "1.4.4",
    "next": "15.1.6",
    "next-auth": "4.24.11",
    "next-navigation-guard": "0.1.2",
    "next-nprogress-bar": "2.4.4",
