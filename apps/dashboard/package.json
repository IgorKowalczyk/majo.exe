    "@majoexe/util": "workspace:*",
    "@mdx-js/loader": "3.1.0",
    "@mdx-js/react": "3.1.0",
    "@next-auth/prisma-adapter": "1.0.7",
    "@next/mdx": "15.1.6",
    "@odiffey/discord-markdown": "3.3.0",
    "@playwright/test": "1.50.1",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-tooltip": "1.1.7",
    "@tanstack/react-table": "8.20.6",
