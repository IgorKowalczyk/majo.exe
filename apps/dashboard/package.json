{
  "name": "@majoexe/dashboard",
  "version": "6.0.0",
  "description": "Dashboard for Majo.exe built on Next.js and Tailwind.css",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "deploy": "pnpm run build && pnpm run start",
    "format": "prettier . --write --cache --ignore-unknown --plugin=prettier-plugin-tailwindcss",
    "format:check": "prettier . --check --cache",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@headlessui/react": "1.7.17",
    "@headlessui/tailwindcss": "0.2.0",
    "@heroicons/react": "2.0.18",
    "@majoexe/config": "workspace:*",
    "@majoexe/database": "workspace:*",
    "@majoexe/util": "workspace:*",
    "@mdx-js/loader": "3.0.0",
    "@mdx-js/react": "3.0.0",
    "@next-auth/prisma-adapter": "1.0.7",
    "@next/mdx": "14.0.2",
    "@tippyjs/react": "4.2.6",
    "@vercel/analytics": "1.1.1",
    "autoprefixer": "10.4.16",
    "client-only": "0.0.1",
    "clsx": "2.0.0",
    "cobe": "0.6.3",
    "geist": "1.1.0",
    "globby": "14.0.0",
    "js-file-download": "0.4.12",
    "json-2-csv": "5.0.1",
    "mini-svg-data-uri": "1.4.4",
    "next": "14.0.2",
    "next-auth": "4.24.5",
    "next-nprogress-bar": "2.1.2",
    "next-themes": "0.2.1",
    "node-fetch": "3.3.2",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-colorful": "5.6.1",
    "react-dom": "18.2.0",
    "react-infinite-scroller": "1.2.6",
    "react-spring": "9.7.3",
    "react-table": "7.8.0",
    "react-twemoji": "0.5.0",
    "react-wrap-balancer": "1.1.0",
    "recharts": "2.9.3",
    "server-only": "0.0.1",
    "sharp": "0.32.6",
    "sonner": "1.2.0",
    "swr": "2.2.4",
    "tailwind-gradient-mask-image": "1.1.0",
    "tailwind-merge": "2.0.0",
    "tailwindcss": "3.3.5",
    "tailwindcss-text-fill": "0.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.0.2",
    "@prisma/nextjs-monorepo-workaround-plugin": "5.6.0",
    "@tailwindcss/typography": "0.5.10",
    "eslint-config-next": "14.0.2",
    "eslint-plugin-import": "2.28.1",
    "prettier-plugin-tailwindcss": "0.5.7"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorkowalczyk/majo.exe.git",
    "directory": "apps/dashboard"
  },
  "keywords": [
    "Majo.exe",
    "Dashboard",
    "React",
    "Next.js",
    "Tailwind.css",
    "web-dashboard"
  ],
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igorkowalczyk/majo.exe/issues"
  },
  "homepage": "https://github.com/igorkowalczyk/majo.exe#readme",
  "packageManager": "pnpm@8.10.5"
