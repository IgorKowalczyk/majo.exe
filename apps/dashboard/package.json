{
  "name": "@majoexe/dashboard",
  "version": "6.0.0",
  "description": "Dashboard for Majo.exe built on Next.js and Tailwind.css",
  "scripts": {
    "start": "dotenvx run -f .env -f ../../packages/database/.env --overload -- next start",
    "dev": "dotenvx run -f .env -f ../../packages/database/.env --overload -- next dev --turbopack",
    "build": "dotenvx run -f .env -f ../../packages/database/.env --overload -- next build",
    "deploy": "pnpm run build && pnpm run start",
    "format": "prettier . --write --cache --ignore-unknown",
    "format:check": "prettier . --check --cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@bprogress/next": "3.2.12",
    "@majoexe/config": "workspace:*",
    "@majoexe/database": "workspace:*",
    "@majoexe/util": "workspace:*",
    "@mdx-js/loader": "3.1.1",
    "@mdx-js/react": "3.1.1",
    "@next-auth/prisma-adapter": "1.0.7",
    "@next/mdx": "16.0.7",
    "@odiffey/discord-markdown": "3.3.0",
    "@prisma/nextjs-monorepo-workaround-plugin": "7.1.0",
    "@radix-ui/react-accordion": "1.2.12",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tooltip": "1.2.8",
    "@t3-oss/env-core": "0.13.8",
    "@t3-oss/env-nextjs": "0.13.8",
    "@tanstack/react-table": "8.21.3",
    "@vercel/speed-insights": "1.3.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "cobe": "0.6.5",
    "discord-api-types": "0.38.36",
    "discord-giveaways": "6.0.1",
    "framer-motion": "12.23.25",
    "geist": "1.5.1",
    "json-2-csv": "5.5.10",
    "lucide-react": "0.556.0",
    "next": "16.0.7",
    "next-auth": "4.24.13",
    "next-axiom": "1.9.3",
    "next-navigation-guard": "0.2.0",
    "react": "19.2.3",
    "react-colorful": "5.6.1",
    "react-dom": "19.2.3",
    "react-infinite-scroller": "1.2.6",
    "react-spring": "10.0.3",
    "react-wrap-balancer": "1.1.1",
    "recharts": "3.5.1",
    "server-only": "0.0.1",
    "sharp": "0.34.5",
    "sonner": "2.0.7",
    "tw-animate-css": "1.4.0",
    "validator": "13.15.23",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@majoexe/typescript-config": "workspace:*",
    "@next/bundle-analyzer": "16.0.7",
    "@tailwindcss/postcss": "4.1.17",
    "@tailwindcss/typography": "0.5.19",
    "@types/mdx": "2.0.13",
    "@types/ms": "2.1.0",
    "@types/react": "19.2.7",
