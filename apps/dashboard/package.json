{
  "name": "@majoexe/dashboard",
  "version": "6.0.0",
  "description": "Dashboard for Majo.exe built on Next.js and Tailwind.css",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "deploy": "pnpm run build && pnpm run start",
    "format": "prettier --write . --cache --ignore-unknown --plugin=prettier-plugin-tailwindcss",
    "format:check": "prettier . --check --cache",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@headlessui/react": "1.7.16",
    "@headlessui/tailwindcss": "0.1.3",
    "@heroicons/react": "2.0.18",
    "@igorkowalczyk/is-browser": "5.0.0",
    "@majoexe/config": "workspace:*",
    "@majoexe/database": "workspace:*",
    "@majoexe/util": "workspace:*",
    "@next-auth/prisma-adapter": "1.0.7",
    "@tippyjs/react": "4.2.6",
    "@vercel/analytics": "1.0.1",
    "@vercel/og": "0.5.9",
    "autoprefixer": "10.4.14",
    "client-only": "0.0.1",
    "clsx": "2.0.0",
    "cobe": "0.6.3",
    "globby": "13.2.2",
    "mini-svg-data-uri": "1.4.4",
    "next": "13.4.12",
    "next-auth": "4.22.3",
    "next-nprogress-bar": "2.0.3",
    "next-themes": "0.2.1",
    "node-fetch": "3.3.2",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-colorful": "5.6.1",
    "react-dom": "18.2.0",
    "react-infinite-scroller": "1.2.6",
    "react-spring": "9.7.2",
    "react-table": "7.8.0",
    "react-twemoji": "0.5.0",
    "recharts": "2.7.2",
    "server-only": "0.0.1",
    "sharp": "0.32.4",
    "swr": "2.2.0",
    "tailwind-gradient-mask-image": "1.0.0",
    "tailwind-merge": "1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-text-fill": "0.2.0"
  },
  "devDependencies": {
    "@majoexe/eslint-config": "workspace:*",
    "@next/bundle-analyzer": "13.4.12",
    "@prisma/nextjs-monorepo-workaround-plugin": "5.0.0",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "0.4.1"
  },
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorkowalczyk/majo.exe.git",
    "directory": "apps/dashboard"
  },
  "keywords": [
    "Majo.exe",
    "Dashboard",
    "React",
    "Next.js",
    "Tailwind.css",
    "web-dashboard"
  ],
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igorkowalczyk/majo.exe/issues"
  },
  "homepage": "https://github.com/igorkowalczyk/majo.exe#readme",
  "packageManager": "pnpm@8.6.10"
}
