  "devDependencies": {
    "@majoexe/typescript-config": "workspace:*",
    "@next/bundle-analyzer": "15.1.6",
    "@prisma/nextjs-monorepo-workaround-plugin": "6.3.1",
    "@tailwindcss/typography": "0.5.16",
    "@types/mdx": "2.0.13",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "10.4.20",
