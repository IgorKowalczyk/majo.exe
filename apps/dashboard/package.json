  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorkowalczyk/majo.exe.git",
    "directory": "apps/dashboard"
  },
  "keywords": [
    "Majo.exe",
    "Dashboard",
    "React",
    "Next.js",
    "Tailwind.css",
    "web-dashboard"
