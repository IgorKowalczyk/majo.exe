{
  "name": "@majoexe/dashboard",
  "version": "6.0.0",
  "description": "Dashboard for Majo.exe built on Next.js and Tailwind.css",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "docker:build": "cd ../.. & docker build -t majoexe/dashboard:latest -f ./apps/dashboard/Dockerfile .",
    "deploy": "pnpm run build && pnpm run start",
    "format": "prettier . --write --cache --ignore-unknown --plugin=prettier-plugin-tailwindcss",
    "format:check": "prettier . --check --cache",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@headlessui/react": "2.1.9",
    "@headlessui/tailwindcss": "0.2.1",
    "@majoexe/config": "workspace:*",
    "@majoexe/database": "workspace:*",
    "@majoexe/util": "workspace:*",
    "@mdx-js/loader": "3.0.1",
    "@mdx-js/react": "3.0.1",
    "@next-auth/prisma-adapter": "1.0.7",
    "@next/mdx": "14.2.14",
    "@odiffey/discord-markdown": "3.3.0",
    "@tippyjs/react": "4.2.6",
    "@vercel/analytics": "1.3.1",
    "@vercel/speed-insights": "1.0.12",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "cobe": "0.6.3",
    "discord-api-types": "0.37.101",
    "framer-motion": "11.11.1",
    "geist": "1.3.1",
    "js-file-download": "0.4.12",
    "json-2-csv": "5.5.6",
    "lucide-react": "0.447.0",
    "mini-svg-data-uri": "1.4.4",
    "next": "14.2.14",
    "next-auth": "4.24.8",
    "next-nprogress-bar": "2.3.13",
    "react": "18.3.1",
    "react-colorful": "5.6.1",
    "react-dom": "18.3.1",
    "react-infinite-scroller": "1.2.6",
    "react-spring": "9.7.4",
    "react-table": "7.8.0",
    "react-wrap-balancer": "1.1.1",
    "recharts": "2.12.7",
    "server-only": "0.0.1",
    "sharp": "0.33.5",
    "sonner": "1.5.0",
    "validator": "13.12.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.2.14",
    "@prisma/nextjs-monorepo-workaround-plugin": "5.20.0",
    "@tailwindcss/typography": "0.5.15",
    "autoprefixer": "10.4.20",
    "postcss": "8.4.47",
    "prettier-plugin-tailwindcss": "0.6.8",
    "tailwind-gradient-mask-image": "1.2.0",
    "tailwind-merge": "2.5.3",
    "tailwindcss": "3.4.13",
    "tailwindcss-text-fill": "0.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorkowalczyk/majo.exe.git",
    "directory": "apps/dashboard"
  },
  "keywords": [
    "Majo.exe",
    "Dashboard",
    "React",
    "Next.js",
    "Tailwind.css",
    "web-dashboard"
  ],
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igorkowalczyk/majo.exe/issues"
  },
  "homepage": "https://github.com/igorkowalczyk/majo.exe#readme",
  "packageManager": "pnpm@9.12.1"
}
