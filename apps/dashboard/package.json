{
  "name": "@majoexe/dashboard",
  "version": "6.0.0",
  "description": "Dashboard for Majo.exe built on Next.js and Tailwind.css",
  "scripts": {
    "start": "next start",
    "dev": "next dev --turbo",
    "build": "next build",
    "docker:build": "cd ../../ && docker build --tag majoexe/dashboard:latest --file ./apps/dashboard/Dockerfile .",
    "deploy": "pnpm run build && pnpm run start",
    "format": "prettier . --write --cache --ignore-unknown",
    "format:check": "prettier . --check --cache",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@headlessui/tailwindcss": "0.2.1",
