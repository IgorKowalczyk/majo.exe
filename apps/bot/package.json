    "@majoexe/util": "workspace:*",
    "@napi-rs/canvas": "0.1.66",
    "@observablehq/plot": "0.6.16",
    "@top-gg/sdk": "3.1.6",
    "chalk": "5.4.1",
    "cheerio": "1.0.0",
    "coloras": "2.0.2",
    "d3": "7.9.0",
    "decode-gif": "1.0.1",
    "discord-giveaways": "6.0.1",
    "discord.js": "14.17.3",
    "discordbotlist": "1.1.1",
    "dotenv": "16.4.7",
    "figlet": "1.8.0",
