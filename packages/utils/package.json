{
  "name": "@majoexe/util",
  "version": "6.0.0",
  "description": "Shared utility functions for Majo.exe",
  "type": "module",
  "exports": {
    "./database": "./database/index.js",
    "./database/*": "./database/*",
    "./embeds": "./embeds/index.js",
    "./embeds/*": "./embeds/*",
    "./functions/automod": "./functions/automod/index.ts",
    "./functions/automod/*": "./functions/automod/*",
    "./functions/files/*": "./functions/files/*",
    "./functions/guild": "./functions/guild/index.js",
    "./functions/guild/*": "./functions/guild/*",
    "./functions/user": "./functions/user/index.js",
    "./functions/user/*": "./functions/user/*",
    "./functions/util": "./functions/util/index.js",
    "./functions/util/*": "./functions/util/*",
    "./images": "./images/index.js",
    "./images/*": "./images/*"
  },
  "scripts": {
    "format": "prettier . --write --cache --ignore-unknown",
    "format:check": "prettier . --check --cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@majoexe/config": "workspace:*",
    "@majoexe/database": "workspace:*",
    "@majoexe/typescript-config": "workspace:*",
    "chalk": "5.3.0",
    "discord-api-types": "0.37.105",
    "discord.js": "14.16.3",
    "ms": "2.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorkowalczyk/majo.exe.git",
    "directory": "packages/util"
  },
  "keywords": [
    "Majo.exe",
    "util",
    "shared",
    "utility-functions"
  ],
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igorkowalczyk/majo.exe/issues"
  },
  "homepage": "https://github.com/igorkowalczyk/majo.exe#readme",
  "packageManager": "pnpm@9.14.2"
}
