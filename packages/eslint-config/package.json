{
  "name": "@majoexe/eslint-config",
  "version": "6.0.0",
  "description": "Majo.exe shared eslint config, based on @igorkowalczyk/eslint-config",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.mjs",
      "default": "./index.mjs"
    }
  },
  "type": "module",
  "scripts": {
    "format": "prettier . --write --cache --ignore-unknown",
    "format:check": "prettier . --check --cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@igorkowalczyk/eslint-config": "2.0.2",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-turbo": "1.10.16",
    "globals": "13.23.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorkowalczyk/majo.exe.git",
    "directory": "packages/eslint-config"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "styleguide",
    "style",
    "guide",
    "lint",
    "linting",
    "code-quality"
  ],
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igorkowalczyk/majo.exe/issues"
  },
  "homepage": "https://github.com/igorkowalczyk/majo.exe#readme",
  "packageManager": "pnpm@8.9.2"
}
