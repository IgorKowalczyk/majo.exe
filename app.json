{
 "name": "majo.exe-v13-beta",
 "description": "Majo.exe have almost everything - Fun, Memes, Images, Giveaway, Economy, Anime and NSFW",
 "logo": "https://github.com/IgorKowalczyk/majobot/blob/master/lib/img/readme/avatar.png",
 "repository": "https://github.com/igorkowalczyk/majobot.git",
 "stack": "heroku-20",
 "website": "https://majobot.igorkowalczyk.repl.co",
 "success_url": "/",
 "buildpacks": [
  {
   "url": "heroku/nodejs"
  }
 ],
 "env": {
  "TOKEN": {
   "description": "Bot token",
   "required": true
  },
  "PREFIX": {
   "description": "Bot prefix (eg. !majo)",
   "required": true
  },
  "AMEAPI": {
   "description": "Amethyste api token",
   "required": true
  },
  "COOKIES": {
   "description": "Youtube cookies",
   "required": false
  },
  "DOMAIN": {
   "description": "Website domain (same as your heroku app name!)",
   "required": false
  },
  "ID": {
   "description": "Discord Bot ID",
   "required": true
  },
  "CONTACT_WEBHOOK_TOKEN": {
   "description": "Contact form webhook token",
   "required": false
  },
  "CONTACT_WEBHOOK_ID": {
   "description": "Contact form webhook id",
   "required": false
  },
  "STATUS_WEBHOOK_TOKEN": {
   "description": "Status webhook token",
   "required": false
  },
  "STATUS_WEBHOOK_ID": {
   "description": "Status webhook id",
   "required": false
  },
  "DASHBOARD": {
   "description": "Dashboard launch config (Boolean | true/false)",
   "required": true
  },
  "SESSION_SECRET": {
   "description": "Random sequence of words, letters and numbers",
   "required": false
  },
  "SECRET": {
   "description": "Bot client secret",
   "required": false
  },
  "ANALYTICS": {
   "description": "Google Analytics tracking ID",
   "required": false
  },
  "MYSQL_DATABASE": {
   "description": "MYSQL Database name",
   "required": true
  },
  "MYSQL_HOST": {
   "description": "MYSQL Host Name",
   "required": true
  },
  "MYSQL_PASSWORD": {
   "description": "MYSQL user password",
   "required": true
  },
  "MYSQL_USER": {
   "description": "MYSQL User name (user must have access to database!)",
   "required": true
  }
 },
 "keywords": ["discord", "discord-bot", "node-js", "web-dashboard", "bot"]
}
