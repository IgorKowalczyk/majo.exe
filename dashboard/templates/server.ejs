<%- include("partials/head", { bot, user, path, description, title: "Majobot", twitter, url, image }) %>
<div class="content">
<% if (guild.icon) {
 iconurl = "https://cdn.discordapp.com/icons/" + guild.id + "/" + guild.icon + ".png??size=512";
} else {
 iconurl = "https://cdn.glitch.com/82fe990a-7942-42e3-9790-39807ccdb9f6%2Ficon-404-dark.png?v=1602427904949";
}
%>

<div class="guild">
<img class="server-image" src="<%= iconurl %>" alt="<% guild.name %>"/>
<p class="server-name"><%= guild.name %></p>
<p class="server-description"><%= guild.description %></p>
  
<%= guild.joined_at%>

<%= guild.banner %>

<p class="region"><svg xmlns="http://www.w3.org/2000/svg" height="24" viewBox="0 0 24 24" width="24"><path d="M0 0h24v24H0z" fill="none"/><path d="M12 2C8.13 2 5 5.13 5 9c0 5.25 7 13 7 13s7-7.75 7-13c0-3.87-3.13-7-7-7zm0 9.5c-1.38 0-2.5-1.12-2.5-2.5s1.12-2.5 2.5-2.5 2.5 1.12 2.5 2.5-1.12 2.5-2.5 2.5z"/></svg> <%= guild.region %></p>
</div>

 
  
  
  
<style>
.region {
display: flex;
-webkit-box-align: center;
align-items: center;
}

.server-image {
width: 119px;
margin-left: 0;
margin-right: 0;
height: 119px;
border-radius: 50%;
}
  
.guild {
text-align: center;
}
</style>
<%- include("partials/footer") %>
